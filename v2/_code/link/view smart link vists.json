{
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{access_token}}",
                "type": "string"
            }
        ]
    },
    "method": "GET",
    "header": [],
    "url": {
        "raw": "{{endpoint}}api/v2/link/urls/:id/visits",
        "host": [
            "{{endpoint}}api"
        ],
        "path": [
            "v2",
            "link",
            "urls",
            ":id",
            "visits"
        ],
        "variable": [
